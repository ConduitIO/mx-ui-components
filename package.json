{
  "name": "mx-ui-components",
  "version": "1.9.3",
  "description": "Meroxa UI component library",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ConduitIO/mx-ui-components.git"
  },
  "license": "Apache-2.0",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test-coverage": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "storybook": "ember serve & start-storybook -p 6006 -s dist",
    "build:dev": "ember build && build-storybook -s dist",
    "build:prod": "ember build && build-storybook -s dist",
    "chromatic": "npx chromatic --build-script-name 'build:prod'",
    "build-storybook": "yarn run build:prod --"
  },
  "dependencies": {
    "ember-auto-import": "^2.5.0",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-keyboard": "8.2.0",
    "ember-named-blocks-polyfill": "^0.2.4",
    "ember-root-url": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@changesets/changelog-github": "^0.4.6",
    "@changesets/cli": "^2.24.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.3",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@meroxa/ui-base": "https://github.com/meroxa/ui-base.git#v2.0.0",
    "@storybook/addon-a11y": "^6.3.13",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.3.13",
    "@storybook/addon-knobs": "^6.3.13",
    "@storybook/addon-links": "^6.3.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/ember": "^6.3.13",
    "@storybook/ember-cli-storybook": "^0.5.0",
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-ember-modules-api-polyfill": "^2.12.0",
    "babel-plugin-htmlbars-inline-precompile": "3",
    "broccoli-asset-rev": "^3.0.0",
    "chromatic": "^6.2.1",
    "core-js": "^3.9.1",
    "ember-a11y-testing": "^4.0.6",
    "ember-cli": "~4.0.1",
    "ember-cli-code-coverage": "^1.0.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-postcss": "^7.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^2.0.3",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^3.1.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.0.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-tooltips": "^3.5.1",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "sinon": "^9.2.4",
    "tailwindcss": "^3.0.22",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">= 12.20.* || 14.* || >= 16"
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "hawk": "^8.0.1"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
